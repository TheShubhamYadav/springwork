<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="laptopcharger" class="com.autowiring.beans.LaptopCharger" autowire="byType"></bean>
	<bean id="mobilecharger" class="com.autowiring.beans.MobileCharger" autowire="constructor"></bean>
	<bean id="poweradaptor1" class="com.autowiring.beans.PowerAdaptor" primary="true"></bean>
	<bean id="poweradaptor2" class="com.autowiring.beans.PowerAdaptor"></bean>

</beans>
