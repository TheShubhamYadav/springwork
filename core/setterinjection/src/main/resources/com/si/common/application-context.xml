<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="ac" class="com.si.beans.AirConditioner">
		<property name="remote" ref="remote"></property>
	</bean>
	<bean id="tv" class="com.si.beans.Television">
		<property name="remote" ref="remote"></property>
	</bean>
	<bean id="heater" class="com.si.beans.Heater">
		<property name="remote" ref="remote"></property>
	</bean>
	<bean id="remote" class="com.si.beans.Remote" />
</beans>
